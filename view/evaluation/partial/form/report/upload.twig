{% if form %}
    {% do form.prepare() %}

    {{ form().openTag(form)|raw }}

    {{ ztbformelement(form.get('excel')) }}
    <div class="row mb-3">
        <div class="offset-sm-3 col-sm-9">
            {{ ztbformelement(form.get('upload')) }}
        </div>
    </div>

    {{ form().closeTag()|raw }}
{% endif %}