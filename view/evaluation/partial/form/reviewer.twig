{% do form.prepare() %}
{{ form().openTag(form)|raw }}

{% set projectReviewer = form.get('evaluation_entity_reviewer') %}

{{ ztbformelement(projectReviewer.get('type')) }}
{{ contactformelement(projectReviewer.get('contact')) }}
{{ formelement(form.get('csrf')) }}

<div class="row mb-3">
    <div class="offset-sm-3 col-sm-9">
        {{ ztbformelement(form.get('submit')) }}
        {% if form.has('delete') %}{{ ztbformelement(form.get('delete')) }}{% endif %}
        {{ ztbformelement(form.get('cancel')) }}
    </div>
</div>

{{ form().closeTag()|raw }}